{
    "name": "thopra/styleguide-template-default",
    "description": "Default template for thopra/styleguide",
    "type": "thopra-styleguide-template",
    "repositories": [
        { "type": "vcs", "url": "https://github.com/thopra/styleguide.git" },
        { "type": "vcs", "url": "https://github.com/thopra/styleguide-template-installer.git" }
    ],
    "keywords": [
        "kss",
        "styleguide",
        "css documentation"
    ],
    "license": "MIT",
    "require": {
    	"thopra/styleguide":"dev-development",
    	"thopra/styleguide-template-installer":"*",
    	"robloach/component-installer": "*",
        "components/font-awesome": "4.*",
        "components/jqueryui": "1.*",
        "components/bootstrap": "3.3.*"
    },
    "authors": [
        {
            "name": "Thomas Prangenberg",
            "email": "t@thomas-prangenberg.de",
            "homepage": "http://www.thomas-prangenberg.de"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "component-dir": "Assets/contrib"
    }
}
